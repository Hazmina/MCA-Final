FROM            Student AS a INNER JOIN
                         ClassDetail AS b ON a.Classid = b.ClassId INNER JOIN
                         Division AS c ON a.Divid = c.DivId
WHERE        (a.Stud_Id NOT IN
                             (SELECT        a.Stud_Id
                               FROM            FeeCollection AS FeeCollection_1
                               WHERE        (Classid = a.Classid) AND (Studid = a.Stud_Id) AND (LastMonth &gt;= @Month)))</CommandText>
