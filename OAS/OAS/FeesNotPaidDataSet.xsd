                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        b.ClassId, c.DivName, a.General_Regis_No, a.Student_Name, a.Phone_No, ISNULL
                             ((SELECT        LastMonthSpel
                                 FROM            FeeCollection
                                 WHERE        (FeeCollectionId =
